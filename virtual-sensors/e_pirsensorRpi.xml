 <virtual-sensor name="e_pirsensorRpi" priority="11">
    <processing-class>
        <class-name>gsn.endymion.EndymionProcessingClass</class-name>
        <output-structure>
            <field name="motion" type="int" />
        </output-structure>
    </processing-class>
    <description>
        Getting temperature.
    </description>
    <life-cycle pool-size="10" />
    <addressing>
        <predicate key="geographical">ZARI</predicate>
        <predicate key="LATITUDE">45.80000</predicate>
        <predicate key="LONGITUDE">16.000000</predicate>
		<predicate key="owner">Endymion</predicate>
    </addressing>
    <storage history-size="8760h" />
    <streams>
        <stream name="input1">
            <source alias="source1" sampling-rate="100" storage-size="1">
                <address wrapper="endymion_vsensor_wrapper">
                    <predicate key="GSNId">192.168.5.84:22001</predicate>
                    <predicate key="vSensorName">pirsensor</predicate>
                </address>
                <query>select * from wrapper</query>
            </source>
            <query>select * from source1</query>
        </stream>
    </streams>
</virtual-sensor>