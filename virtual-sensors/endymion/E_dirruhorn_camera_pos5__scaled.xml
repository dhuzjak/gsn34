<virtual-sensor name="dirruhorn_camera_pos5__scaled" priority="10">
<processing-class>
    <class-name>gsn.endymion.EndymionProcessingClass</class-name>
    <output-structure>
        <field name="device_id" type="int" />
        <field name="jpeg_scaled" type="binary:image/jpeg" />
    </output-structure>
</processing-class>
<description>Collects data from Endymion</description>
<life-cycle pool-size="10" />
<addressing>
    <predicate key="geographical">Endymion - GSN</predicate>
    <predicate key="LATITUDE">46.12222222222222</predicate>
    <predicate key="LONGITUDE">7.847222222222222</predicate>
    <predicate key="owner">RASIP - Endymion</predicate>
</addressing>
<storage history-size="8760h" />
<streams>
    <stream name="input1">
        <source alias="source1" sampling-rate="1" storage-size="1">
            <address wrapper="endymion_vsensor_image_wrapper">
                <predicate key="GSNId">data.permasense.ch:80</predicate>
                <predicate key="vSensorName">dirruhorn_camera_pos5__scaled</predicate>
            </address>
            <query>SELECT * FROM wrapper</query>
        </source>
        <query>SELECT * FROM source1</query>
    </stream>
</streams>
</virtual-sensor>